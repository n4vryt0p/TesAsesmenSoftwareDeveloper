@page "{handler?}"
@model IndexModel
@{
    ViewData["Title"] = "Master";
}
@section styles{
    <!--datatable css-->
    <link href="~/css/dx.light.css" rel="stylesheet" />
}
@Html.AntiForgeryToken()
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header d-flex align-items-center">
                <h5 class="card-title mb-0 flex-grow-1">Product Manager</h5>
                @* <div id="file-uploader"></div> *@
            </div>
            <div class="card-body">
                <div id="masterGrid"></div>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <!--datatable js-->
    <script src="~/js/dx.all.js"></script>
    <script src="~/js/dx.aspnet.data.js"></script>
    <script src="~/assets/libs/jspdf/jspdf.umd.min.js"></script>
    <script src="~/js/master-page.js"></script>
}